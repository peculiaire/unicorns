[
    {
        "name": "unicorns",
        "image": "peculiaire/smartdm:latest",
        "portMappings": [
            {
                "containerPort": 8080,
                "hostPort": 8080
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-region": "us-east-2",
                "awslogs-group": "/ecs/unicorns",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }
]
